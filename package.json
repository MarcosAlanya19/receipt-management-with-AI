{
  "name": "nestjs-encore-example",
  "private": true,
  "version": "0.0.1",
  "description": "NestJS + Encore Example",
  "license": "MPL-2.0",
  "type": "module",
  "scripts": {
    "postinstall": "npx prisma generate --schema=src/prisma/schema.prisma",
    "dev": "encore run",
    "build": "encore build",
    "start": "node dist/main.js",
    "start:dev": "encore dev --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit",
    "prisma:generate": "npx prisma generate --schema=src/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=src/prisma/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --force --schema=src/prisma/schema.prisma"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "tsx src/prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^6.10.0",
    "currency.js": "^2.0.4",
    "encore.dev": "^1.48.4",
    "joi": "^17.13.3",
    "knex": "^3.1.0",
    "luxon": "^3.6.1",
    "openai": "^5.5.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rimraf": "5.0.5",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/luxon": "^3.6.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "encore.dev": "^1.48.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
